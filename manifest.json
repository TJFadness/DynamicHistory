{
  "manifest_version": 2,

  "name": "DynamicHistory",
  "author": "nulldev",
  "description": "Automagically delete history based on the keywords on the page!",
  "version": "1.1",
  "options_page": "options.html",

  "permissions": [
    "activeTab",
    "tabs",
    "<all_urls>",
    "history",
    "storage"
  ],
  "icons": { "16": "icon16.png",
    "32": "icon32.png",
           "48": "icon48.png",
          "128": "icon128.png" },
  "background": {
    "scripts": ["background.js"]
  },
  "browser_action": {}
}
